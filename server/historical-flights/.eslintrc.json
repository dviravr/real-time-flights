{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "max-len": 0,
        "new-cap": 0,
        "object-curly-spacing": 0,
        "no-unused-vars": 0,
        "semi": ["error", "always"],
        "quotes": ["error", "single"]
    }
}
